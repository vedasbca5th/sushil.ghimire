@{
    ViewData["Title"]= "Employee";
}
@model List<Employee>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Address</th>
      <th scope="col">Age</th>
    <th scope="col">Salary</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var empoyee in Model){
 <tr>
    <th scope="row">1</th>
    <td>@empoyee.Name</td>
    <td>@empoyee.Address</td>
    <td>@empoyee.Age</td>
    <td>@empoyee.Salary</td>
    </tr>
    }
  </tbody>
</table>